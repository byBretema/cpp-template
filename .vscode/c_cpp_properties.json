        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "~/.conan2/**",
                "~/.conan2/p/**/p/include/"
            ],
            "compilerPath": "cl.exe",
            "cStandard": "c17",
            "cppStandard": "c++20"
        },
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "~/.conan2/**",
                "~/.conan2/p/**/p/include/"
